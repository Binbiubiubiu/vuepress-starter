{
  "name": "vuepress-starter",
  "version": "1.0.0",
  "description": "a vuepress template",
  "main": "index.js",
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "cz": "git cz",
    "release": "standard-version",
    "github-release": "conventional-github-releaser -p angular -r 0"
  },
  "repository": {
    "type": "github",
    "url": "github+https://github.com/Wildlifes/vuepress-starter.git"
  },
  "keywords": [],
  "author": "lvbin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Wildlifes/vuepress-starter/issues"
  },
  "homepage": "https://github.com/Wildlifes/vuepress-starter#readme",
  "devDependencies": {
    "commitizen": "^4.0.3",
    "conventional-github-releaser": "^3.1.3",
    "cz-conventional-changelog": "3.0.2",
    "husky": "^3.1.0",
    "standard-version": "^7.0.1",
    "vuepress": "^1.2.0"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
